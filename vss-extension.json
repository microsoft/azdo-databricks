{
    "manifestVersion": 1,
    "id": "azdo-databricks",
    "name": "DevOps for Azure Databricks",
    "version": "0.1.12",
    "publisher": "riserrad",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/logo.png"
    },
    "description": "A set of Pipeline tasks to help add DevOps practices to a Databricks development cycle.",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/azdo-databricks"
    },
    "files": [
        {
            "path": "tasks/DeployNotebooksTask"
        },
        {
            "path": "tasks/ConfigureDatabricks"
        },
        {
            "path": "tasks/ExecuteNotebook"
        },
        {
            "path": "tasks/WaitExecution"
        },
        {
            "path": "tasks/Scala/ConfigureNetRc"
        },
        {
            "path": "tasks/Scala/StartCluster"
        },
        {
            "path": "tasks/Scala/InstallScalaTools"
        },
        {
            "path": "tasks/Scala/InstallSpark"
        },
        {
            "path": "tasks/Scala/UninstallCodeFromCluster"
        }
    ],
    "contributions": [
        {
            "id": "azdo-databricks-deploynotebooks",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/DeployNotebooksTask"
            }
        },
        {
            "id": "azdo-databricks-configuredatabricks",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/ConfigureDatabricks"
            }
        },
        {
            "id": "azdo-databricks-executenotebook",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/ExecuteNotebook"
            }
        },
        {
            "id": "azdo-databricks-waitexecution",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/WaitExecution"
            }
        },
        {
            "id": "azdo-databricks-configurenetrc",
            "type":"ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Scala/ConfigureNetRc"
            }
        },
        {
            "id": "azdo-databricks-startcluster",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Scala/StartCluster"
            }
        },
        {
            "id": "azdo-databricks-installscalatools",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Scala/InstallScalaTools"
            }
        },
        {
            "id": "azdo-databricks-installspark",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Scala/InstallSpark"
            }
        },
        {
            "id": "azdo-databricks-uninstallcodefromcluster",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/Scala/UninstallCodeFromCluster"
            }
        }
    ]
}